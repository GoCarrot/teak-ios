{
  "scripts": {
    "docs": "doxygen && doxygen2adoc --config .doxygen2adoc.yml build --clean",
    "changelog": "doxygen2adoc changelog --clean --input docs/modules/changelog/versions/ --output docs/modules/changelog/partials --nav docs/modules/changelog/pages/changelog.adoc"
  },
  "dependencies": {
    "doxygen2adoc": "https://github.com/GoCarrot/doxygen2adoc"
  }
}
